{{#if nav}}
<div class="yui3-menu yui3-menu-open">
    <a class="yui3-menu-title" href="#">YUI CSS</a>
    <a href="#menu" class="yui3-menu-link">
        <img src="/img/navicon-png2x.png" width="20"/>
    </a>
    <ul>
      {{#each nav}}
        <li class="{{#if isActive}}yui3-menu-active{{/if}}">
            <a href="{{url}}">{{label}}</a>
        </li>
      {{/each}}
    </ul>
</div>
{{/if}}
